SELECT 
  CURRENT_ORGANIZATION_NAME() AS ORGANIZATION_NAME,
  CURRENT_ACCOUNT() AS ACCOUNT_NAME,
  CURRENT_REGION() AS ACCOUNT_REGION,
  CURRENT_ACCOUNT_NAME() AS ACCOUNT_LOCATOR;



select * from onpoint_id.forecast_day
limit 10;



ALTER WAREHOUSE COMPUTE_WH SUSPEND;


ALTER WAREHOUSE COMPUTE_WH
SET
  WAREHOUSE_SIZE = 'MEDIUM',
  AUTO_SUSPEND = 300,
  AUTO_RESUME = TRUE;








-- Step 1: Create the database
CREATE DATABASE BOOTCAMP;

-- Step 2: Create the schema inside the BOOTCAMP database
CREATE SCHEMA BOOTCAMP.RAW_LAYER;





  CREATE WAREHOUSE COMPUTE_WH1
WITH
  WAREHOUSE_SIZE = 'SMALL'
  WAREHOUSE_TYPE = 'STANDARD'
  MIN_CLUSTER_COUNT = 3
  MAX_CLUSTER_COUNT = 3
  AUTO_SUSPEND = 240
  AUTO_RESUME = TRUE
  INITIALLY_SUSPENDED = TRUE;


